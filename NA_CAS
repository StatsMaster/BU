    |_|_|  \__,_|_| |_|___/_| \___/|_|  |_| |_| |_|\__,_|\__|_|\___/|_| |_|___/
                                                                               

	> STEP 1: Create Percentile Bins using PROC RANK
	> STEP 2: Determine cut point for within BIN
	> STEP 3: Output the SAS code to an external file for possible later use
;


%Macro PCT_TRANS (var=,dsn=,op=,filez=);
proc rank data = &dsn groups = 100 out = out ; var &var. ; ranks bin ; run;
proc means data = out noprint nway ; class bin ; var &var. ; output out = EndPts max = max ; run ;
filename &filez. "&op\&filez..sas" ;
data _Null_ ; file &filez. ; set  endpts end = last;
 if _n_ = 1 then put "select;";
 if not last then do; put " when (&var. <= " max ") &filez. =" bin ";"; end;
 else if last then do; put "otherwise &filez. =" bin ";"; put "end;"; end;
run;
%mend PCT_TRANS ;

/* 
Macro Form
	%PCT_TRANS (var=,dsn=,op=,filez=) 

Application of Results in a datastep
	%include RTC / source2; %include PTC / source2;

R = Rolling 12 Months
P = Period Month

C = Total Cost
V = Volume
D = DIAO
O = O Fees
T = Transactions

B = Bins

*/

%Macro PCT_TRANS (var=,dsn=,op=,filez=,FileName=);
proc rank data = &dsn groups = 100 out = out ; var &var. ; ranks bin ; run;
proc means data = out noprint nway ; class bin ; var &var. ; output out = EndPts max = max ; run ;
filename &FileName. "&op\&filez..sas" ;
data _Null_ ; file &FileName. ; set  endpts end = last;
 if _n_ = 1 then put "select;";
 if not last then do; put " when (&var. <= " max ") &filez. =" bin ";"; end;
 else if last then do; put "otherwise &filez. =" bin ";"; put "end;"; end;
run;
%mend PCT_TRANS ;

%macro Percent_Transform;
%let DSN=valid_Clean ;
%let var_num_list = 
PROJMTHVOL
PRD_TRN
PRD_VOL
PRD_DIA
PRD_BC_DIA
PRD_BC_D
PRD_BC_I
PRD_BC_A
PRD_O
PRD_TOT_CST
RTM_TRN
RTM_VOL
RTM_DIA
RTM_O
RTM_TOT_CST
MMF
PRD_DIAO
RTM_DIAO
RTM_CST_to_VOL
PRD_CST_to_VOL
PRD_DIA_to_VOL
RTM_DIA_to_VOL
RTM_FEE_to_CST
PRD_FEE_to_CST
CRR_VOL_PCH
CRR_TRN_PCH
CRR_DIA_PCH
CRR_O_PCH
CRR_DIAO_PCH
TIM
DTD
ELR
VAS_CNT
;
%let var_cnt = 33 ;
%do x = 1 %to &var_cnt.;
	%let var = %scan(&var_num_list.,&x.,' ');
%PCT_TRANS 
(	var		 =	&VAR.	,  
	filez	 = 	B_&VAR.	, 
	FileName =  BIN_&X.	,
	DSN		 =	&DSN.	, 
	op		 =	C:\Users\srseibe\Desktop\Task\Nomis Risk Score\sas code)
%end;
data &DSN. ;
 set &DSN. ; 
 %include BIN_1 ; %include BIN_2 ; %include BIN_3 ; %include BIN_4 ; %include BIN_5 ;     
 %include BIN_6 ; %include BIN_7 ; %include BIN_8 ; %include BIN_9 ; %include BIN_10;     

 %include BIN_11 ; %include BIN_12 ; %include BIN_13 ; %include BIN_14 ; %include BIN_15 ;     
 %include BIN_16 ; %include BIN_17 ; %include BIN_18 ; %include BIN_19 ; %include BIN_20 ;     

 %include BIN_21 ; %include BIN_22 ; %include BIN_23 ; %include BIN_24 ; %include BIN_25 ;     
 %include BIN_26 ; %include BIN_27 ; %include BIN_28 ; %include BIN_29 ; %include BIN_30 ;     

 %include BIN_31 ;
 %include BIN_32 ;
 %include BIN_33 ;
 
run ;
%Mend Percent_Transform;

options NoMPrint NoSymbolgen;
%Percent_Transform
options MPrint Symbolgen;


/* Apply Features */*;


data loc_srs.valid_v2 ;
 set valid_Clean ;

 &IND_GRP_SC.  &GEO_GRP_SC. &CSR_GRP_SC. &PTY_GRP_SC. 

 if IND_GRP = 4  then IND_GRP = 9;
 if IND_GRP = .  then IND_GRP = 1;

 if GEO_GRP = .  then GEO_GRP = 7;
 if GEO_GRP = 1  then GEO_GRP = 8;

 if CSR_GRP = 2  then CSR_GRP = 9;
 if CSR_GRP = .  then CSR_GRP = 9;

run ;


