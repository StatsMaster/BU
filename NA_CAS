Proc Logistic Data = loc_srs.Train_v2 ;
	Model Attrition ( event = '1' ) = &var. / stb ;
	ods output ParameterEstimates = PE_SOLO_&x. Association = A ; Run ;
proc sql ; select nValue2 into :C from A where Label2 = 'c' ; quit ;
data PE_SOLO_&x. ; set PE_SOLO_&x. ; C = &C. ; run ;
%end ;
Proc Sql ;
	Select distinct memname into :PE_DS seperated by ' '
	from dictionary.tables where upcase(libname) = 'WORK' and memname like 'PE_SOLO_%' ;
Quit ;
data loc_srs.PE_5 ; set &PE_DS. ; if variable = 'Intercept' then delete ; drop df ; run ;
Proc DataSets lib=work NoList NoDetails ; Delete &PE_DS.; quit;

