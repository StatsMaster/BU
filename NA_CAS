/* Screening Vars */

/* Variables Screening */

%Macro FScreen;
%let w = 1 ;
%let DSN  = loc_srs.Train_v2; %put &dsn.;
%let RSP  = Attrition ;
%let Nvar = %sysfunc(countW(&var_lst.)); %put &nvar;

ods listing close;
ods output
spearmancorr = spearman
hoeffdingcorr = hoeffding;

Proc Corr data = &DSN. spearman hoeffding rank ;
	var  &var_lst.;
	with &RSP. ; 
Run;

ods listing;

data spearman1 (keep = variable scorr spvalue ranksp);
	length variable $ 32 ;
	set spearman;
	array best(*) best1 -- best&nvar. ;
	array r(*) r1 -- r&nvar. ;
	array p(*) p1 -- p&nvar. ;
	do i = 1 to dim(best) ;
		variable = best(i) ;
		scorr = r(i) ; 
		spvalue = p(i) ;
		ranksp = i ;
	output;
	end;
run;

data hoeffding1 (keep=variable hcorr hpvalue rankho);
length variable $ 32;
set hoeffding;
array best(*) best1 -- best&nvar. ;
array r(*) r1 -- r&nvar. ; 
array p(*) p1 -- p&nvar. ;
do i = 1 to dim(best);
variable = best(i) ;
hcorr = r(i) ;
hpvalue = p(i) ;
rankho = i ;
output;
end;
run;

proc sort data = spearman1 ; by variable; run;

proc sort data = hoeffding1 ; by variable; run;

data correlations_&w. ;
merge spearman1 hoeffding1;
by variable ;
run;

proc sort data = correlations_&w.;
by ranksp;
run;

%mend;
%FScreen

proc print data = correlations_1 label ;
var variable ranksp rankho scorr spvalue hcorr hpvalue;
label 
	ranksp = 'Spearman rank of variables'
	scorr = 'Spearman Correlations'
	spvalue = 'Spearman p-value'
	rankho = 'Hoeffding rank of variables'
	hcorr = 'Hoeffding Correlation'
	hpvalue = 'Hoeffding p-value'; 
run;

proc VarClus 
	data 		= loc_srs.Train_v2
	maxeigen 	= 0.7
	outtree		= ForTree
	MaxClusters = 100
	Short ;
	Var &var_lst. ;
	Ods output  
		ClusterQuality 	= Summary 
		RSquare 		= Clusters ;
Run ;

/* Top 100 Variables (Cluster 100 and pick top H-Ranks (Hoeff. picks up on non-linear links */

