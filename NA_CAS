		X_CRR_TRN_PCH_CRR_O_PCH
		X_PRD_TRN_RTM_DIA_to_VOL
		RTM_FEE_to_CST
	/ STB RSQ ;
	Score 
		data 		= loc_srs.valid_v2 
		out	 		= Score_v1
		PriorEvent	= 0.0521
	;
Run ;

/* ROC Optimal Point */
 
proc logistic data=loc_srs.Train_v2;
	Class GEO_GRP IND_GRP CSR_GRP
	/ Param = Reference ;
	Model Attrition (event='1') = 
		GEO_GRP
		CSR_GRP
		IND_GRP
		PRD_CST_to_VOL_LN
		X_PRD_CST_to_VOL_DTD
		X_RTM_DIA_CRR_TRN_PCH
		X_RTM_DIA_ELR
		X_RTM_TRN_CRR_VOL_PCH
		CRR_VOL_PCH_LN
		X_CRR_TRN_PCH_CRR_O_PCH
		X_PRD_TRN_RTM_DIA_to_VOL
		RTM_FEE_to_CST
		RTM_FEE_to_CST_X3
	/ outroc=o rocepts=0;
 output out=model1 pred=phat;
run;
proc sql;
 create table annodat as
 	select *, 
	'draw' 		as Function, 
	1 			as Line, 
	'red' 		as color, 
	_1mspec_ 	as X, 
	_sensit_ 	as Y, 
	'2' 		as xsys, 
	'2' 		as ysys
 from 	
	(select *, 
	sqrt((0-_1mspec_)**2+(1-_sensit_)**2) 		as Dist, 
	min(sqrt((0-_1mspec_)**2+(1-_sensit_)**2)) 	as Min 
	from work.o)
 where dist=min;
 title 'Point where Distance is Minimized';
 select dist, _sensit_, _1mspec_, _PROB_ Length=8 Format 10.8
 from annodat;
quit;
title;
data example;
 retain xsys ysys hsys '2';
 function = 'move'; x=0; y=1; output;
run;
data newanno;
 set example annodat;
run;
proc gplot data=o anno=newanno;
 plot _Sensit_*_1mspec_;
run;
quit;

/* Validation Scoring */

Proc Logistic Data = loc_srs.Train_v2 outmodel = mod.CAS_MODEL;
	Class GEO_GRP IND_GRP CSR_GRP
	/ Param = Reference ;
	Model Attrition (event='1') = 
		GEO_GRP
		CSR_GRP
		IND_GRP
		PRD_CST_to_VOL_LN
		X_PRD_CST_to_VOL_DTD
		X_RTM_DIA_CRR_TRN_PCH
		X_RTM_DIA_ELR
		X_RTM_TRN_CRR_VOL_PCH
		CRR_VOL_PCH_LN
		X_CRR_TRN_PCH_CRR_O_PCH
		X_PRD_TRN_RTM_DIA_to_VOL
		RTM_FEE_to_CST
	/ STB RSQ ;
	Score 
		data 		= Valid_v2 
		out	 		= Score_v1
		PriorEvent	= 0.0521
	;
Run ;

proc logistic data = Score_v1 des;
	model attrition (event='1') = P_1 ;
run;

data Score_v2;
 set Score_v1;
 if P_1 >= 0.24567393 then Pred_Attrition = 1; else Pred_Attrition = 0;
run;

Proc Freq data = Score_v2 ;
	Tables Attrition*Pred_Attrition;
Run;

Proc Rank
	data = Score_v2 
	out  = Score_v3
	Groups = 10 ;
	Var P_1 ;
	Ranks CAS;
Run;

data Score_v3; set Score_v3; CAS = CAS+1;

Proc Freq data = Score_v3 ;
	Tables CAS*Attrition / nocol nopercent nofreq;
Run;

Proc Means data = Score_v3 mean;
	class CAS ;
	var P_1 ;
run;

Title "CAS = 1" ; 
Proc Freq data = Score_v3 ;
	where CAS = 1;
	tables State SIC_DESCRIPTION  CSR ;
Run ;

Title "CAS = 5" ; 
Proc Freq data = Score_v3 ;
	where CAS = 5;
	tables State SIC_DESCRIPTION  CSR ;
Run ;

Title "CAS = 10" ; 
Proc Freq data = Score_v3 ;
	where CAS = 10;
	tables State SIC_DESCRIPTION  CSR ;
Run ;

proc means data = score_v3 mean ;
	Class cas ;
	var   rtm_diao rtm_o tim PRD_CST_to_VOL CRR_VOL_PCH BNK_wPRT P_1;
run;

proc means data = score_v3 mean median p75;
	where prd_vol > 0;
	Class cas ;
	var   CRR_VOL_PCH ;
run;

proc means data = score_v3 mean median p75;
	where prd_vol > 0;
	Class cas ;
	var   PRD_CST_to_VOL ;
run;

proc means data = score_v3 mean median p75;
	Class cas ;
	var   RTM_FEE_to_CST ;
run;

proc means data = score_v3 mean median p75;
	Class cas ;
	var   tim ;
run;

proc means data = score_v3 mean ;
	where prd_vol > 0;
	Class cas ;
	var   PRD_CST_to_VOL ;
run;


Proc Print data = score_v3  (obs=10);
	where cas=10 ;
run;




