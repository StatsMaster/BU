CRR_DIA_PCH
CRR_O_PCH
CRR_DIAO_PCH
TIM
DTD
ELR
VAS_CNT
;
%let var_cnt = 33 ;
%do x = 1 %to &var_cnt.;
	%let var = %scan(&var_num_list.,&x.,' ');
%PCT_TRANS 
(	var		 =	&VAR.	,  
	filez	 = 	B_&VAR.	, 
	FileName =  BIN_&X.	,
	DSN		 =	&DSN.	, 
	op		 =	C:\Users\srseibe\Desktop\Task\Nomis Risk Score\sas code)
%end;
data train_Clean ;
 set train_Clean ; 
 %include BIN_1 ; %include BIN_2 ; %include BIN_3 ; %include BIN_4 ; %include BIN_5 ;     
