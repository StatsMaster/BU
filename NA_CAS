%let IM = 
B_CRR_TRN_PCH
B_DTD
B_PRD_CST_to_VOL
B_PRD_DIA
B_PRD_VOL
CRR_TRN_PCH
CRR_TRN_PCH_X3
PRD_CST_to_VOL_IN
PRD_CST_to_VOL_LN
PRD_TOT_CST_IN
PRD_TRN_X2
PRD_VOL
PRD_VOL_SR
RTM_DIA_to_VOL_LN
TIM
VAS_CNT_SR
X_CRR_O_PCH_CRR_TRN_PCH
X_CRR_O_PCH_PRD_FEE_to_CST
X_CRR_TRN_PCH_PRD_DIA
X_CRR_TRN_PCH_PRD_FEE_to_CST
X_CRR_TRN_PCH_PRD_O
X_CRR_TRN_PCH_RTM_TOT_CST
X_CRR_VOL_PCH_RTM_TRN
X_DTD_PRD_VOL
X_ELR_CRR_TRN_PCH
X_MMF_CRR_DIAO_PCH
X_MMF_PRD_BC_I
X_MMF_PRD_DIAO
X_PRD_BC_D_PRD_DIAO
X_PRD_BC_D_RTM_DIA_to_VOL
