

/**********************************//**********************************//**********************************/
/**********************************//**********************************//**********************************/
/**********************************//* Elavon Attrition Risk Score v2 *//**********************************/
/**********************************//**********************************//**********************************/
/**********************************//**********************************//**********************************/


/* 
	Customer Attrition Score  

	Target Variable: 				The Customer is closed within three months of an active month

	Sample Definition 
	Exclutions 
		> Powertrack
		> Customer who would not call into customer service: group_ 	not in ('RM', 'AIRLN', 'HCARE', 'HOSP')
		> Cash Advance Customers: sic_code not in ('6010', '6011')

	Analysis Time Period: 	 		between '01APR2013' and '01MAR2014'
	
	Potential Input Variables
		> ACQ Channel
		> Account Tenure
		> Industry, through SIC Codes and MIS/MISS
		> Year Estblished Tenure
		> Account Tenure in Months 
		
		> Bankcard VOL, TRN, DIA
		> Sum of Fees "O"
		> MID Avg. Ticket VOL/TRN
		> MID Avg. Eff. Rate DIA/VOL
		> %Change of Eff. Rate vs. the Avg. Eff. Rate of Their MIS
		> Ratio of (TC Rate / Eff. Rate)
		> Four Month Trend of, TC Rate
		> Min Fee Payer in the Past Three Months
		> Projected Monthly Volume
		
	Analysis Method
		1.) Compile the Data Frame
		2.) Compile the Oversampled Dataset
		3.) Split the Data into Training & Validation
		4.) Clean Data and impute missing values with median
				a.) Windsorizing
		5.) Variable Transformations
				a.) Percentile Transformation and Rank Transformation
				b.) Ratio's
				c.) Cat Clustering
				d.) LN(), Square Root sqrt(), Inverse 1/x				
				f.) Disctrization (equal widths, equal bins)
		6.) Variable Selection
		7.)  
		
*/

/*

PROC SQL ;
	connect to oracle (user = &U. password = &P. path= 'NEWDW01.WORLD');
		create table Loc_srs.samp_1 as select * from connection to oracle ( select 
			b.mid	  						as MID , 
			b.period  						as Period ,
			z.sponsorid 					as Client_Group ,
			s.assocnumber					as Assc_Num ,
			nvl(s.siccode,'0000') 			as SIC_Code ,
			n.*,
			m.*,
			z.Channel						as Channel ,
			z.rdc							as Sub_Channel ,
			z.Route							as SC_Route ,
			coalesce(p.group_  ,'Null')  	as Rela_Type ,
			coalesce(p.rel_long,'Null')  	as Rela ,

			s.merchstatus_ecp				as STATUS ,
			s.first_deposit_date 			as FDD ,
			s.last_deposit_date  			as LDD ,
			s.date_opened 		 			as OPD ,
			s.date_closed		 			as CLD ,
			c.closecode						as Close_Code ,
			x.category						as Close_Code_CAT ,
			x.description					as Close_Code_DES ,
			s.FEDTAXID ,			 
			s.FUNDINGABA ,
			s.FUNDINGDDA ,			
			k.mbankname 	as DEPOSIT_BANK,
			s.REPCODE ,
			s.projmthvol ,
			i.repcode, 
			i.REPNAME 		as REP_NAME , 
			i.channel 		as REP_Channel,
			z.city 			as City  , 
			z.state_cd		as State , 
			z.zip_cd		as Zip   ,
				
			nvl(b.trancount,		0) 		as PRD_TRN  , 
			nvl(b.netvolume,		0) 		as PRD_VOL  , 
		    nvl(b.stm_discount,	 	0) + nvl(b.stm_surcharge,		0) 		
		  + nvl(b.stm_authrevenue,	0) + nvl(b.stm_ind_plan,		0) 	+ nvl(b.stm_data_cap,		0) 		
		  - nvl(b.tsys_intchg,	 	0) - nvl(b.assesments,		 	0) 			
		  + nvl(b.dscv_discount,	0) - nvl(b.dscv_interchange,	0) 	- nvl(b.dscv_assessments,	0) 
		  + nvl(b.pin_db_discount,	0) - nvl(b.pin_db_interchange,  0)	- nvl(b.pin_db_assessments,	0) 	- nvl(b.pin_db_switch_fee,0) 
											as PRD_DIA ,
		   nvl(b.stm_discount,   	0) + nvl(b.stm_surcharge, 0) - nvl(b.pin_db_auth_income,0)
		 + nvl(b.stm_authrevenue,	0)	+ nvl(b.stm_ind_plan,  0)
		 + nvl(b.stm_data_cap,   	0) - nvl(b.tsys_intchg,   0)	-nvl(b.assesments,0) 
											as PRD_BC_DIA ,
		   nvl(b.stm_discount,   	0) + nvl(b.stm_surcharge, 0) - nvl(b.pin_db_auth_income,0)
		 + nvl(b.stm_authrevenue,	0)	+ nvl(b.stm_ind_plan,  0)
		 + nvl(b.stm_data_cap,   	0) 		as PRD_BC_D ,
		   nvl(b.tsys_intchg,    	0)		as PRD_BC_I ,
		   nvl(b.assesments,	 	0) 		as PRD_BC_A ,
		   nvl(b.stm_feerevenue, 	0) - nvl(b.pos_fee,0)
		   									as PRD_O , 
		   nvl(b.STM_INCOME,		0)		as PRD_TOT_CST  ,

			nvl(r.trancount,	 	0) 		as RTM_TRN  , 
			nvl(r.netvolume,	 	0) 		as RTM_VOL  , 
		   nvl(r.stm_discount,	 	0) + nvl(r.stm_surcharge,		0) 		
		 + nvl(r.stm_authrevenue,	0) + nvl(r.stm_ind_plan,		0) 	+ nvl(r.stm_data_cap,		0) 		
		 - nvl(r.tsys_intchg,	 	0) - nvl(r.assesments,		 	0) 			
		 + nvl(r.dscv_discount,	 	0) - nvl(r.dscv_interchange,	0) 	- nvl(r.dscv_assessments,	0) 
		 + nvl(r.pin_db_discount,	0) - nvl(r.pin_db_interchange, 0)	- nvl(r.pin_db_assessments,	0) 	- nvl(r.pin_db_switch_fee,0) 
											as RTM_DIA 	,
		   nvl(r.stm_feerevenue,0) - nvl(r.pos_fee,0)
		   									as RTM_O 	,  
		   nvl(r.STM_INCOME,		0)   	as RTM_TOT_CST


		from 		browser.merch_tran_info 		b 
		inner join 	bsnsdevl.mid_owner 				z on b.mid = z.mid
		left  join	browser.roll_mti				r on b.mid = r.mid and b.period = r.period
		left  join	bsnsdevl.mid_relationship 		p on b.mid = p.mid
		left  join  browser.merch_demo_info_master	s on b.mid = s.mid
		left  join  dw.merchantmaster				c on b.mid = c.mid 
		left  join  dw.close_code_description		x on c.closecode = x.close_code 
		left  join	BSNSDEVL.REPINFO				i on s.repcode = i.repcode
		left  join  BSNSDEVL.NAICS_LU				n on s.siccode = n.siccode		
		left  join	BSNSDEVL.MARKET_INSIGHT_NAICS	m on n.NAICS_3 = m.NAICS3_CODE
		left  join	dw.ababank						k on s.FUNDINGABA  = k.transroutingnumber and k.country = 'USA'
		left  join	aep.merch_mid					d on b.mid = d.mid
		left  join	aep.merch_mid_addr 				z on d.mid_tag = z.mid_tag and z.addr_type_tag = '2'

		where 		b.period between '01JAN2013'  and '01MAR2014'
		and			z.rdc 		<> 'POWERTRACK' 
		and			z.sponsorid <> '0006'
		and			p.group_ not in 
					('RM', 'AIRLN', 'HCARE', 'HOSP') 
		and			s.siccode not in ('6010', '6011')
		and			p.rel_long not in 
					('TNT AMERICAN PROMOTIONAL EVENTS', 'AUCTION PAY', 'PAMS LUNCHROOM PREMIERES') ) ;
	disconnect from oracle ;
quit;

* Value Added Service Sign Dates ;

PROC SQL ;
	connect to oracle (user = &U. password = &P. path= 'NEWDW01.WORLD');
		create table VAS_SGN as select * from connection to oracle (
		select 	* from dw.mid_product_dates where date_type = 'SGN' );
	disconnect from oracle ;
quit;

proc sort data = vas_sgn ; by mid ; run ;

data VAS_SGN_v2;
 set VAS_SGN ;
 Format SGN_DATE date9. ;
 SGN_DATE = datepart(date_value);
run ;

proc transpose 
	data	=	vas_sgn_v2 
	out		=	vas_sgn_v2 (drop = _name_ _label_) prefix=SGN_ ;
	by MID ;
	ID product_cd;
	var SGN_DATE;
run ;

* Year Established for Business Tenure ;

PROC SQL ;
	connect to oracle (user = &U. password = &P. path= 'NEWDW01.WORLD');		
		create table YE as select * from connection to oracle ( select distinct
		b.mid, a.year_established						
		from 			aep.merch_bus_info 	a
		inner join		aep.merch_app		b on a.app_id = b.app_id );
	disconnect from oracle ;
quit;

* Current PType ;

PROC SQL ;
	connect to oracle (user = &U. password = &P. path= 'NEWDW01.WORLD');		
		create table  ptype as select * from connection to oracle( 
		select distinct SPOEMID as MID, Pricing_Type as PType from RBAILEY.MODEL_APR13 );
	disconnect from oracle ;
quit;

* MIN Payer Fees *;

%Macro MPF;
%do x = 1 %to 15;
data _null_ ;
	Call Symputx
	('month',
	Upcase(Compress("'"||put(intnx('month',"01DEC2012"D,&x.,'B'),date9.)||"'"))) ; 
run ;
%put &month;
PROC SQL ;
	connect to oracle (user = &U. password = &P. path= 'NEWDW01.WORLD');		
		create table MPF_TEMP_&x. as select * from connection to oracle ( 
		Select Distinct a.MID, a.period, max(a.DISC_CHARGED) as MONTLY_Min_Fee
		from 	dw.merchtrandetail a 
		where 	a.chg_type_number 	in ('00099', '0099')
		and 	a.cardtype 			= 'MB'
		and		a.period = &Month.
		Group By a.MID , a.period);
	disconnect from oracle ;
quit;
%end;
Proc Sql;
	Select distinct memname into :MPF_DS seperated by ' '
	from dictionary.tables where upcase(libname) = 'WORK' and memname like 'MPF_TEMP%' ;
Quit;
data MPF;
 set &MPF_DS.;
 run;
%mend MPF;
%MPF

proc sql;
	create table samp_1v2 as select a.*,
		b.MONTLY_MIN_FEE as Monthly_Min_Fee , 
		p.PType	   ,
		c.SGN_DCC  ,
		c.SGN_ECS  ,
		c.SGN_EGC  ,
		c.SGN_AMEX ,
		c.SGN_CKSV ,
		d.Year_Established 

	from 		loc_srs.samp_1 		a
	left join	MPF			b	 on a.MID = b.MID and datepart(a.period) = datepart(b.period)
	left join	VAS_SGN_v2	c	 on a.mid = c.mid
	left join	ptype		p	 on a.mid = p.mid
	left join 	ye 			d 	 on a.mid = d.mid;

quit;

Proc Sort data = samp_1v2 NoDupKey; by MID Period ; run ;
*/


/*

* Copy the new temp set into the perm loc;

	data loc_srs.samp_1; 
	set samp_1v2;
	run ;
*/

*
  _____        __ _              _   _          
 |  __ \      / _(_)            | | | |         
 | |  | | ___| |_ _ _ __   ___  | |_| |__   ___ 
 | |  | |/ _ \  _| | '_ \ / _ \ | __| '_ \ / _ \
 | |__| |  __/ | | | | | |  __/ | |_| | | |  __/
 |_____/ \___|_| |_|_| |_|\___|  \__|_| |_|\___|
 |  __ \                                        
 | |__) |___  ___ _ __   ___  _ __  ___  ___    
 |  _  // _ \/ __| '_ \ / _ \| '_ \/ __|/ _ \   
 | | \ \  __/\__ \ |_) | (_) | | | \__ \  __/   
 |_|  \_\___||___/ .__/ \___/|_| |_|___/\___|   
                 | |                            
                 |_|                            
;

data samp_2 ;
 set loc_srs.samp_1 ;
 Format P Date9. ;
 P = datepart(Period) ;
 if  P < '01APR2013'D then delete;
 /* 1.) ID the Closed MIDs */
 if STATUS in ('C', 'D') and datepart(CLD) ne .  then  Closed = 1; else Closed = 0 ;
 /* 2.) Period 3 months from LDD */
 if Closed then do ;
  PRD_to_CLD = floor (((day(datepart(Period)) < day(datepart(CLD))) - intck('Month',datepart(CLD),datepart(Period)) )) ;  
 end ;
 Format PRE_CLS_PRD Date9. ;
 if PRD_to_CLD = 4 then PRE_CLS_PRD = datepart(Period) ;

 /* 
 	Data sits a MID-Period Level.
 	define the sample for each period, 
 	all mids who will close 3 months from now or later.
 	The mid which close in 3 months are the target we want to find.
 	Remove all MIDs that have already closed or will close before the target.
	We want to comepare the MIDs who will close in 3 months with the MIDs who will
	not close in 3 months or less.	
 */

 if closed and PRD_to_CLD < 1 then delete;
 if Closed and PRD_to_CLD in (1,2,3,4) then Attrition = 1; else Attrition = 0;

run ;

*
   _____                       _ _                        
  / ____|                     | (_)                       
 | (___   __ _ _ __ ___  _ __ | |_ _ __   __ _            
  \___ \ / _` | '_ ` _ \| '_ \| | | '_ \ / _` |           
  ____) | (_| | | | | | | |_) | | | | | | (_| |           
 |_____/ \__,_|_| |_| |_| .__/|_|_|_| |_|\__, |           
                        | |               __/ |           
   __             __  __|_|        _     |___/            
  / _|           |  \/  |         | |    | (_)            
 | |_ ___  _ __  | \  / | ___   __| | ___| |_ _ __   __ _ 
 |  _/ _ \| '__| | |\/| |/ _ \ / _` |/ _ \ | | '_ \ / _` |
 | || (_) | |    | |  | | (_) | (_| |  __/ | | | | | (_| |
 |_| \___/|_|    |_|  |_|\___/ \__,_|\___|_|_|_| |_|\__, |
                                                     __/ |
                                                    |___/ 
;

/* How Rare is the Event ? */

/*
Proc Freq data = samp_2 ;
	Tables Attrition /  missing;
Run;
*/

/* Oversample for the Rare Event, and splitting up the data into training-validation-test */

proc surveyselect NoPrint  
	data 		= samp_2 (where=(Attrition=1)) 
	out			= samp_A1
	method		= srs
	seed		= 20140719
	N			= 50000 ;
run;

proc surveyselect NoPrint  
	data 		= samp_2 (where=(Attrition=0)) 
	out			= samp_A0
	method		= srs
	seed		= 20140719
	N			= 150000 ;
run;

data samp_3;
 set samp_A1 samp_A0;
run;

proc sort data = samp_3 ; by Attrition ; run ;

proc surveyselect NoPrint 
	data		= samp_3 
	out			= develop
	samprate	= .6667
	seed		= 20140719
	outall;
	Strata Attrition ;
run;

/*
Proc Freq data = develop ;
	Tables Attrition*Selected /  missing;
Run;
*/

data train valid ;
 set develop ;

 PRD_DIAO  = sum(PRD_DIA, PRD_o);
 RTM_DIAO  = sum(RTM_DIA, RTM_o);

 Format CSR CSC $50.;
 CSR = compress(Channel)||"-"||compress(Sub_Channel)||"-"||compress(SC_ROUTE) ;
 CSC = compress(Channel)||"-"||compress(Sub_Channel) ;

 * Total Cost / Volume ;
 if RTM_TOT_CST  <= 0 or rtm_vol = 0 then RTM_CST_to_VOL = 0;
 else RTM_CST_to_VOL = abs(rtm_tot_cst) /abs( rtm_vol);

 if PRD_TOT_CST  <= 0 or prd_vol = 0 then PRD_CST_to_VOL = 0;
 else PRD_CST_to_VOL = abs(PRD_tot_cst) /abs(PRD_vol);

 * DIA / VOL - Effective Rate ;
 PRD_DIA_to_VOL = abs(PRD_DIA) /abs(PRD_VOL); if PRD_DIA_to_VOL < 0 then PRD_DIA_to_VOL = 0;
 RTM_DIA_to_VOL = abs(RTM_DIA) /abs(RTM_VOL); if RTM_DIA_to_VOL < 0 then RTM_DIA_to_VOL = 0;

 * O Fees / Total Cost ;
 if RTM_TOT_CST  <= 0 or RTM_O = 0 then RTM_FEE_to_CST = 0;
 else RTM_FEE_to_CST = abs(rtm_o) /abs( rtm_tot_cst);
 if RTM_FEE_to_CST <0  then RTM_FEE_to_CST = 0;

 if PRD_TOT_CST  <= 0 or PRD_O = 0 then PRD_FEE_to_CST = 0;
 else PRD_FEE_to_CST = abs(PRD_O) /abs(PRD_TOT_CST);
 if  PRD_FEE_to_CST < 0 then PRD_FEE_to_CST = 0;

 * Avg. Month vs Current Month ;
 CRR_VOL_PCH  = ((PRD_VOL)  - (RTM_VOL /12)) / (RTM_VOL /12) ; if CRR_VOL_PCH  = . then CRR_VOL_PCH  = 0;  
 CRR_TRN_PCH  = ((PRD_TRN)  - (RTM_TRN /12)) / (RTM_TRN /12) ; if CRR_TRN_PCH  = . then CRR_TRN_PCH  = 0;
 CRR_DIA_PCH  = ((PRD_DIA)  - (RTM_DIA /12)) / (RTM_DIA /12) ; if CRR_DIA_PCH  = . then CRR_DIA_PCH  = 0; 
 CRR_O_PCH    = ((PRD_O)    - (RTM_O   /12)) / (RTM_O   /12) ; if CRR_O_PCH    = . then CRR_O_PCH    = 0;
 CRR_DIAO_PCH = ((PRD_DIAO) - (RTM_DIAO/12)) / (RTM_DIAO/12) ; if CRR_DIAO_PCH = . then CRR_DIAO_PCH = 0;

 * TiM ;
 TID = floor ((intck('day',datepart(OPD), datepart(period)) - (day(datepart(period)) < day(datepart(OPD))))) ;
 TIY = TID / 365 ;
 TIM = TIY*12;

 * Activation Speed - Days till Deposit ;
 DTD = (datepart(FDD) - datepart(OPD));
 if dtd < 0 then dtd = 0;

 * Deposit Bank , does the MID bank with a partner? ;
 if upcase(compress(DEPOSIT_BANK)) in (
'U.S.BANKNATIONALASSOCIATION' ,
'KEYBANKNATIONALASSOCIATION' ,	
'REGIONSBANK' ,
'UNIONBANK,N.A.' ,
'BANKOFTHEWEST' ,
'SUNTRUSTBANK' ,
'FIRSTTENNESSEEBANK,NATIONAL' ,
'BANCOSANTANDERPUERTORICO' ,
'CITYNATIONALBANK' ,
'UMBBANK,N.A.' ,
'WHITNEYNATIONALBANK' ,
'ZIONSFIRSTNATIONALBANK' ,
'UMBBANKCOLORADO,NATIONALAS' ,
'IBERIABANK' ,
'MECHANICSBANK' ,
'UMBNATIONALBANKOFAMERICA' ,
'UMBBANKARIZONA,NATIONALASS' ,
'MECHANICSANDFARMERSBANK' ,
'UNIONBANK' ,
'THEFARMERSANDMECHANICSBANK' ,
'IBERIABANKFSB' ,
'MECHANICSCO-OPERATIVEBANK' ,
'MECHANICSSAVINGSBANK'
 ) then BNK_wPRT = 1; else BNK_wPRT = 0;

 * Estimated Lifetime Revenue ELR ;

 ELR 		=      (abs(RTM_DIAO)/12*TIM);
 
 if Monthly_Min_Fee < 0 then Monthly_Min_Fee = 0;
 Rename Monthly_Min_Fee = MMF;

 if PTYPE = '' THEN PTYPE = 'UNKWN';
 
 If SGN_DCC  <> . and SGN_DCC  <= P then DCC_OPENED  = 1; else DCC_OPENED  = 0;
 If SGN_ECS  <> . and SGN_ECS  <= P then ECS_OPENED  = 1; else ECS_OPENED  = 0;
 If SGN_EGC  <> . and SGN_EGC  <= P then EGC_OPENED  = 1; else EGC_OPENED  = 0;
 If SGN_AMEX <> . and SGN_AMEX <= P then AMEX_OPENED = 1; else AMEX_OPENED = 0;
 If SGN_CKSV <> . and SGN_CKSV <= P then CKSV_OPENED = 1; else CKSV_OPENED = 0;
 VAS_CNT = sum(DCC_OPENED , ECS_OPENED , EGC_OPENED , AMEX_OPENED, CKSV_OPENED);

 if selected then output train ; 
 else output valid ;
run ;

Proc Contents data = train varnum; Run;

/* clean */ * ;

proc means data =train mean median;
	where Year_Established <> . ;
	var Year_Established ;	
run;

data train_Clean;
 set train;
 if Year_Established = . then Year_Established =2003;
 BT_TIY = 2015 - Year_Established;
 PTYPE = compress(upcase(PTYPE));
run;


*
  ______         _                                   
 |  ____|       | |                                  
 | |__ ___  __ _| |_ _   _ _ __ ___                  
 |  __/ _ \/ _` | __| | | | '__/ _ \                 
 | | |  __/ (_| | |_| |_| | | |  __/                 
 |_|__\___|\__,_|\__|\__,_|_|  \___|  _             

   _____						   _   _ 	
  / ____|                         | | (_)            
 | |  __  ___ _ __   ___ _ __ __ _| |_ _  ___  _ __  
 | | |_ |/ _ \ '_ \ / _ \ '__/ _` | __| |/ _ \| '_ \ 
 | |__| |  __/ | | |  __/ | | (_| | |_| | (_) | | | |
  \_____|\___|_| |_|\___|_|  \__,_|\__|_|\___/|_| |_|
                                                     
                                                     
;

%macro trans_level_1;
%let var_num_list = 
PROJMTHVOL *
PRD_TRN *
PRD_VOL *
PRD_DIA *
PRD_BC_DIA *
PRD_BC_D *
PRD_BC_I *
PRD_BC_A *
PRD_O *
PRD_TOT_CST *
RTM_TRN *
RTM_VOL *
RTM_DIA *
RTM_O *
RTM_TOT_CST *
MMF *
PRD_DIAO *
RTM_DIAO *
RTM_CST_to_VOL *
PRD_CST_to_VOL *
PRD_DIA_to_VOL *
RTM_DIA_to_VOL *
RTM_FEE_to_CST *
PRD_FEE_to_CST *
CRR_VOL_PCH *
CRR_TRN_PCH *
CRR_DIA_PCH *
CRR_O_PCH *
CRR_DIAO_PCH *
TIM *
DTD *
ELR *
VAS_CNT
;
%let var_cnt = 33 ;

%do x = 1 %to &var_cnt. ;
%let var = %scan(&var_num_list.,&x.,'*');

%IF &x. = 1 %then %do; 
%let sas_code = 
 Format &var._LN &var._SR &var._X2 &var._X3 &var._IN Comma32.7 %str(;)
 InFormat &var._LN &var._SR &var._X2 &var._X3 &var._IN 32.7 %str(;)
 &var._LN 	=  LOG(&var.) %STR(;) 		if &var._LN = . then &var._LN = 0 %STR(;)
 &var._SR 	= SQRT(&var.) %STR(;)		if &var._SR = . then &var._SR = 0 %STR(;)
 &var._X2 	=     (&var.)**2 %STR(;)	if &var._X2 = . then &var._X2 = 0 %STR(;)	
 &var._X3 	=     (&var.)**3 %STR(;)	if &var._X3 = . then &var._X3 = 0 %STR(;)
 &var._IN 	=   1/(&var.) %STR(;)		if &var._IN = . then &var._IN = 0 %STR(;)
;
%end;
%IF &x. > 1 %then %do; 
%let sas_code = &sas_code.
 Format &var._LN &var._SR &var._X2 &var._X3 &var._IN Comma32.7 %str(;)
 InFormat &var._LN &var._SR &var._X2 &var._X3 &var._IN 32.7 %str(;)
 &var._LN 	=  LOG(&var.) %STR(;) 		if &var._LN = . then &var._LN = 0 %STR(;)
 &var._SR 	= SQRT(&var.) %STR(;)		if &var._SR = . then &var._SR = 0 %STR(;)
 &var._X2 	=     (&var.)**2 %STR(;)	if &var._X2 = . then &var._X2 = 0 %STR(;)	
 &var._X3 	=     (&var.)**3 %STR(;)	if &var._X3 = . then &var._X3 = 0 %STR(;)
 &var._IN 	=   1/(&var.) %STR(;)		if &var._IN = . then &var._IN = 0 %STR(;)
;
%end;
%end;
%Put the SAS code is &sas_code. ;
data train_Clean ;
 set train_Clean ;
 &sas_code. 
run;
%mend trans_level_1;

%macro trans_level_2;
%let var_num_list = 
PROJMTHVOL *
PRD_TRN *
PRD_VOL *
PRD_DIA *
PRD_BC_DIA *
PRD_BC_D *
PRD_BC_I *
PRD_BC_A *
PRD_O *
PRD_TOT_CST *
RTM_TRN *
RTM_VOL *
RTM_DIA *
RTM_O *
RTM_TOT_CST *
MMF *
PRD_DIAO *
RTM_DIAO *
RTM_CST_to_VOL *
PRD_CST_to_VOL *
PRD_DIA_to_VOL *
RTM_DIA_to_VOL *
RTM_FEE_to_CST *
PRD_FEE_to_CST *
CRR_VOL_PCH *
CRR_TRN_PCH *
CRR_DIA_PCH *
CRR_O_PCH *
CRR_DIAO_PCH *
TIM *
DTD *
ELR *
VAS_CNT
;
%let var_cnt = 33 ;
%do x = 1 %to 33;
%let var = %scan(&var_num_list.,&x.,'*');
%do j = 2 %to %eval(&var_cnt.-1);
%let cross_var = %scan(&var_num_list.,&j.,'*');
%IF &x. = 1 %then %do; 
%let Cross_Var_Code =  X_&var._&cross_var. = (&var.)*(&cross_var.) %str(;) ;
%end;
%IF &x. > 1 %then %do; 
%let Cross_Var_Code = &Cross_Var_Code X_&var._&cross_var. = (&var.)*(&cross_var.) %str(;) ;
%end;
%end;
%end;
%Put The X-Var Code is &Cross_Var_Code. ;
data train_Clean ; set train_Clean ; &Cross_Var_Code. run;
%mend trans_level_2;

options NoMPrint NoSymbolgen;
%trans_level_1
%trans_level_2
options MPrint Symbolgen;


