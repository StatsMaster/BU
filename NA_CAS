 |  ____|       | |                                  
 | |__ ___  __ _| |_ _   _ _ __ ___                  
 |  __/ _ \/ _` | __| | | | '__/ _ \                 
 | | |  __/ (_| | |_| |_| | | |  __/                 
 |_|__\___|\__,_|\__|\__,_|_|  \___|  _             

   _____						   _   _ 	
  / ____|                         | | (_)            
 | |  __  ___ _ __   ___ _ __ __ _| |_ _  ___  _ __  
 | | |_ |/ _ \ '_ \ / _ \ '__/ _` | __| |/ _ \| '_ \ 
 | |__| |  __/ | | |  __/ | | (_| | |_| | (_) | | | |
  \_____|\___|_| |_|\___|_|  \__,_|\__|_|\___/|_| |_|
                                                     
                                                     
;

%macro trans_level_1;
%let var_num_list = 
PROJMTHVOL *
PRD_TRN *
PRD_VOL *
PRD_DIA *
PRD_BC_DIA *
PRD_BC_D *
PRD_BC_I *
PRD_BC_A *
PRD_O *
PRD_TOT_CST *
RTM_TRN *
RTM_VOL *
RTM_DIA *
RTM_O *
RTM_TOT_CST *
MMF *
PRD_DIAO *
RTM_DIAO *
RTM_CST_to_VOL *
PRD_CST_to_VOL *
PRD_DIA_to_VOL *
RTM_DIA_to_VOL *
RTM_FEE_to_CST *
PRD_FEE_to_CST *
CRR_VOL_PCH *
CRR_TRN_PCH *
CRR_DIA_PCH *
CRR_O_PCH *
CRR_DIAO_PCH *
TIM *
DTD *
ELR *
VAS_CNT
;
%let var_cnt = 33 ;

%do x = 1 %to &var_cnt. ;
%let var = %scan(&var_num_list.,&x.,'*');

%IF &x. = 1 %then %do; 
%let sas_code = 
 Format &var._LN &var._SR &var._X2 &var._X3 &var._IN &var._EX Comma32.7 %str(;)
 InFormat &var._LN &var._SR &var._X2 &var._X3 &var._IN &var._EX      32.7 %str(;)
 &var._LN 	=  LOG(&var.) %STR(;) 		if &var._LN = . then &var._LN = 0 %STR(;)
 &var._SR 	= SQRT(&var.) %STR(;)		if &var._SR = . then &var._SR = 0 %STR(;)
 &var._X2 	=     (&var.)**2 %STR(;)	if &var._X2 = . then &var._X2 = 0 %STR(;)	
 &var._X3 	=     (&var.)**3 %STR(;)	if &var._X3 = . then &var._X3 = 0 %STR(;)
 &var._IN 	=   1/(&var.) %STR(;)		if &var._IN = . then &var._IN = 0 %STR(;)
;
%end;
%IF &x. > 1 %then %do; 
%let sas_code = &sas_code.
 Format &var._LN &var._SR &var._X2 &var._X3 &var._IN &var._EX Comma32.7 %str(;)
 InFormat &var._LN &var._SR &var._X2 &var._X3 &var._IN &var._EX      32.7 %str(;)
 &var._LN 	=  LOG(&var.) %STR(;) 		if &var._LN = . then &var._LN = 0 %STR(;)
 &var._SR 	= SQRT(&var.) %STR(;)		if &var._SR = . then &var._SR = 0 %STR(;)
 &var._X2 	=     (&var.)**2 %STR(;)	if &var._X2 = . then &var._X2 = 0 %STR(;)	
 &var._X3 	=     (&var.)**3 %STR(;)	if &var._X3 = . then &var._X3 = 0 %STR(;)
 &var._IN 	=   1/(&var.) %STR(;)		if &var._IN = . then &var._IN = 0 %STR(;)
;
%end;
%end;
data Valid_Clean ;
 set Valid_Clean ;
 &sas_code. 
run;
%mend trans_level_1;

%macro trans_level_2;
%let var_num_list = 
PROJMTHVOL *
PRD_TRN *
PRD_VOL *
PRD_DIA *
PRD_BC_DIA *
PRD_BC_D *
PRD_BC_I *
PRD_BC_A *
PRD_O *
PRD_TOT_CST *
RTM_TRN *
RTM_VOL *
RTM_DIA *
RTM_O *
RTM_TOT_CST *
MMF *
PRD_DIAO *
RTM_DIAO *
RTM_CST_to_VOL *
PRD_CST_to_VOL *
PRD_DIA_to_VOL *
RTM_DIA_to_VOL *
RTM_FEE_to_CST *
PRD_FEE_to_CST *
CRR_VOL_PCH *
CRR_TRN_PCH *
CRR_DIA_PCH *
CRR_O_PCH *
CRR_DIAO_PCH *
TIM *
DTD *
ELR *
VAS_CNT
;
%let var_cnt = 33 ;
%do x = 1 %to 33;
%let var = %scan(&var_num_list.,&x.,'*');
%do j = 2 %to %eval(&var_cnt.-1);
%let cross_var = %scan(&var_num_list.,&j.,'*');
%IF &x. = 1 %then %do; 
%let Cross_Var_Code =  X_&var._&cross_var. = (&var.)*(&cross_var.) %str(;) ;
%end;
%IF &x. > 1 %then %do; 
%let Cross_Var_Code = &Cross_Var_Code X_&var._&cross_var. = (&var.)*(&cross_var.) %str(;) ;
%end;
%end;
%end;
data Valid_Clean ; set Valid_Clean ; &Cross_Var_Code. run;
%mend trans_level_2;

options NoMPrint NoSymbolgen;
%trans_level_1
%trans_level_2
options MPrint Symbolgen;


*
   _____      _                        _           _  
  / ____|    | |                      (_)         | | 
 | |     __ _| |_ ___  __ _  ___  _ __ _  ___ __ _| | 
 | |    / _` | __/ _ \/ _` |/ _ \| '__| |/ __/ _` | | 
 | |___| (_| | ||  __/ (_| | (_) | |  | | (_| (_| | | 
  \_____\__,_|\__\___|\__, |\___/|_|  |_|\___\__,_|_| 
                       __/ |                          
  /\//\//\//\//\//\//\//\//\//\//\//\//\//\//\//\//\/|
 |/\//\//\//\//\//\//\//\//\//\//\//\//\//\//\//\//\/ 
  / ____| |         | |          (_)                  
 | |    | |_   _ ___| |_ ___ _ __ _ _ __   __ _       
 | |    | | | | / __| __/ _ \ '__| | '_ \ / _` |      
 | |____| | |_| \__ \ ||  __/ |  | | | | | (_| |      
  \_____|_|\__,_|___/\__\___|_|  |_|_| |_|\__, |      
                                           __/ |      
                                          |___/       
;


%Macro CAT_CLUSTERING(DSN=,RSP=,CAT=,NewVar=);
%Global &NewVar._GRP_SC;
proc means data = &dsn. noprint nway; class &cat. ; var &rsp. ; output out = level mean = prop; run;
proc cluster data = level method = ward outtree = fortree ; freq _freq_ ; var prop ; id &cat. ; ods output clusterHistory = Cluster; run ;
proc freq data = &dsn. noprint; tables &cat.*&rsp. / chisq; output out = chi(keep = _pchi_) chisq; run;
data cutoff; if _n_ = 1 then set chi; set cluster ; chisquare = _pchi_*rsquared; degfree = numberofclusters-1; logpvalue = logsdf('CHISQ',chisquare,degfree); run;
proc plot data = cutoff ; plot logpvalue*numberofclusters / vpos = 30; run; quit;
proc sql; select NumberOfClusters into :ncl from cutoff having logpvalue = min(logpvalue); quit;
proc tree data = fortree  h = rsq nclusters = &ncl.  out = clus; id &cat.; run;
proc sort data = clus; by clusname; run; proc print data = clus; by clusname; id clusname; run;
Proc SQL; Create Table &DSN._CATS as select a.*, b.Cluster as &NewVar._GRP from &DSN.  a left join clus b on a.&Cat. = b.&Cat. ; Quit;
Title "New Clusted Version of &CAT.";
Proc Freq data = &DSN._CATS ; tables &NewVar._GRP*&RSP. / nocol nofreq nopercent;
Proc Freq data = &DSN._CATS ; tables &NewVar._GRP / nopercent nocum; Run ; Title;
data clus_code; set clus; format sas_code $75. ; if _n_ = 1 then do;
sas_code =      "IF &CAT. = '"||compress(&CAT.)||"' then  &NewVar._GRP = '"||compress(put(Cluster,5.))||"' ; " ; end;
else if _n_ > 1 then do; sas_code = "Else IF &CAT. = '"||compress(&CAT.)||"' then  &NewVar._GRP = '"||compress(put(Cluster,5.))||"' ; " ; end; run; 
proc sql; select SAS_CODE into :&NewVar._GRP_SC separated by ' ' from clus_code; quit;
%Put The sas code for createing the &NewVar._GRP is &&&NewVar._GRP_SC. ;
%Mend CAT_CLUSTERING ;

* %CAT_CLUSTERING(DSN=,RSP=,CAT=,NewVar=) ;

*
  _____                        _   _ _                                         
 |  __ \                      | | (_) |                                        
 | |__) |__ _ __ ___ ___ _ __ | |_ _| | ___                                    
 |  ___/ _ \ '__/ __/ _ \ '_ \| __| | |/ _ \                                   
 | |  |  __/ | | (_|  __/ | | | |_| | |  __/                                   
 |_|___\___|_|_ \___\___|_|_|_|\__|_|_|\___|______ ______ ______ ______ ______ 
